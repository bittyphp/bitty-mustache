{
    "name": "bittyphp/bitty-mustache",
    "description": "A Mustache template view for Bitty.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Kingsley",
            "email": "bizurkur@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "bittyphp/bitty-view": "^1.0",
        "mustache/mustache": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bitty\\View\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitty\\Tests\\View\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
